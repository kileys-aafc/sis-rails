{
"DocumentType":"http://opengis.net/JSON/template",
"Title":"List Frameworks",
"Description":"List of spatial frameworks available from this web application."
"ParentResources": [
]
"DependentResources": [
	{
	"Title":"List Versions",
	"Description":"List available versions for a specified framework."
	"UriTemplate","/vector/{framework}/versions.json",
	}
]
"TemplateParameters": {
	"framework":[ 
		<%for framework in @frameworks do%>{
		"Value":"<%=framework.framework%>",
		"Title":{
			"en":"<%=framework.title_en%>",
			"fr":"<%=framework.title_fr%>"
			}
		}<%if framework.id != @frameworks[-1].id then%>,<%end%><%end%>
	]
}
}
